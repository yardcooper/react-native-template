{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:device": "react-native run-android -- --device",
    "ios:device": "react-native run-ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts-check": "tsc",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "postinstall": "cd ios && pod install",
    "extract-loc": "formatjs extract 'src/**/!(*.d).{ts,tsx,js,jsx}' --out-file './src/intl/translations/en.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-loc": "formatjs compile-folder ./src/intl/translations ./src/intl/locales",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/stack": "^6.3.11",
    "@reduxjs/toolkit": "^1.9.0",
    "react": "18.2.0",
    "react-intl": "^6.2.5",
    "react-native": "0.71.2",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@formatjs/cli": "^5.1.12",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-formatjs": "^10.3.35",
    "eslint": "^8.19.0",
    "eslint-config-universe": "^10.0.0",
    "eslint-plugin-formatjs": "^4.3.9",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  }
}
